generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  ADMIN
  DOCTOR
  RECEPTIONIST
}

model User {
  id        Int      @id @default(autoincrement())
  username  String
  password  String
  level     Role
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
